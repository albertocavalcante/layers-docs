      [
        "code",
        null,
        "backButtonText"
      ],
      " config to ",
      [
        "code",
        null,
        "Do Not Go Back"
      ],
      ", the ",
      [
        "code",
        null,
        "ion-back-button"
      ],
      " default text would still default to ",
      [
        "code",
        null,
        "Go Back"
      ],
