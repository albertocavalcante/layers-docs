    " ",
    [
      "p",
      null,
      "Running an app on an iOS device requires a provisioning profile. If a provisioning profile has not been created yet follow these directions:"
    ],
    " ",
    [
      "ol",
      null,
      " ",
      [
        "li",
        null,
        [
          "p",
          null,
          [
            "strong",
            null,
            "Set the ",
            [
              "stencil-route-link",
              {
                "url": "/docs/reference/glossary#package-id"
              },
              "Package ID"
            ],
            "."
          ]
        ],
        " ",
        [
          "p",
          null,
          " For Capacitor, open the ",
          [
            "code",
            null,
            "capacitor.config.json"
          ],
          " file and modify the ",
          [
            "code",
            null,
            "appId"
          ],
          " property."
        ],
        " ",
        [
          "p",
          null,
          " For Cordova, open the ",
          [
            "code",
            null,
            "config.xml"
          ],
          " file and modify the ",
          [
            "code",
            null,
            "id"
          ],
          " attribute of the root element, ",
          [
            "code",
            null,
            "<widget>"
          ],
          ". See ",
          [
            "a",
            {
              "href": "https://cordova.apache.org/docs/en/latest/config_ref/#widget"
            },
            "the Cordova documentation"
          ],
          " for more information."
        ],
        " "
      ],
      " ",
      [
        "li",
        null,
        [
          "p",
          null,
          [
            "strong",
            null,
            "Open the project in ",
            [
              "b",
              null,
              "Xcode"
            ],
            "."
          ]
        ],
        " ",
        [
          "p",
          null,
          " For Capacitor, run the following to open the app in Xcode:"
        ],
        " ",
        [
          "docs-code",
          {
            "language": "shell"
          },
          " ",
          [
            "pre",
            null,
            [
              "code",
              null,
              [
                "span",
                {
                  "class": "token prompt"
                },
                " $ "
              ],
              "ionic capacitor ",
              [
                "span",
                {
                  "class": "token function"
                },
                "open"
              ],
              " ios"
