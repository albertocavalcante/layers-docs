        null,
        [
          "p",
          null,
          [
            "strong",
            null,
            "In ",
            [
              "b",
              null,
              "Project navigator"
            ],
            ", select the project root to open the project editor. Under the ",
            [
              "strong",
              null,
              "Identity"
            ],
            " section, verify that the Package ID that was set matches the Bundle Identifier."
