      " version ",
      [
        "code",
        null,
        "7.1.0"
      ],
      " or higher and ",
      [
        "code",
        null,
        "cordova-android"
      ],
      " ",
      [
        "code",
        null,
        "6.3.0"
      ],
      " or higher. To install latest ",
      [
        "code",
        null,
        "cordova"
      ],
      " run:"
