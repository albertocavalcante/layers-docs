    " ",
    [
      "docs-code",
      {
        "language": "shell"
      },
      " ",
      [
        "pre",
        null,
        [
          "code",
          null,
          " ionic cordova plugin ",
          [
            "span",
            {
              "class": "token function"
            },
            "add"
          ],
          " cordova-plugin-facebook4 --variable ",
          [
            "span",
            {
              "class": "token assign-left variable"
            },
            "APP_ID"
          ],
          [
            "span",
            {
              "class": "token operator"
            },
            "="
          ],
          [
            "span",
            {
              "class": "token string"
            },
            "\"123456789\""
