      "docs-code",
      {
        "language": "shell"
      },
      " ",
      [
        "pre",
        null,
        [
          "code",
          null,
          [
            "span",
            {
              "class": "token prompt"
            },
            "$ "
          ],
          "ionic cap run ios -l --external\n",
          [
            "span",
            {
              "class": "token prompt"
            },
            "\n$ "
          ],
          "ionic cap run android -l --external"
        ]
      ],
      " "
    ],
    " ",
    [
